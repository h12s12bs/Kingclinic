<!DOCTYPE html>
@model List<Kingclinic.Models.Reservation>
<html lang="en">
<head>

    <title>Health - Medical Website Template</title>
    <!--

    Template 2098 Health

    http://www.tooplate.com/view/2098-health

    -->
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="Tooplate">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/animate.css">
    <link rel="stylesheet" href="~/Content/owl.carousel.css">
    <link rel="stylesheet" href="~/Content/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Content/custom-style.css">


    <!-- MAIN CSS -->
    <link rel="stylesheet" href="~/Content/tooplate-style.css">

</head>
<body id="top" data-spy="scroll" data-target=".navbar-collapse" data-offset="50">

    <!-- PRE LOADER -->
    <section class="preloader">
        <div class="spinner">

            <span class="spinner-rotate"></span>

        </div>
    </section>


    <!-- ABOUT -->
    <section id="about">
        <div class="container">
            <div class="row">

                <div class="col-md-6 col-sm-6">
                    <div class="about-info">
                        <h2 class="wow fadeInUp" data-wow-delay="0.4s">Welcome to <i class="fa fa-h-square"></i>光明診所</h2>
                        <div class="wow fadeInUp" data-wow-delay="0.6s">
                            <h3><i class="fa fa-hospital-o fa-2x"></i>&nbsp;&nbsp;本院門診</h3>
                            <ul class="about">

                                <li>
                                    <p class="about">骨科</p>
                                </li>
                                <li>
                                    <p class="about">外科</p>
                                </li>
                                <li>
                                    <p class="about">家庭醫學科</p>
                                </li>
                            </ul>
                        </div>
                        <figure class="profile wow fadeInUp" data-wow-delay="1s">
                            <img src="~/images/Fundercirclewhite.png" class="img-responsive" alt="">
                            <figcaption>
                                <h3>張光明 醫師</h3>
                                <p>&nbsp;&nbsp;院長 榮總專科醫師主治</p>
                                <ul>
                                    <li>
                                        <p class="about">體外震波疼痛治療</p>
                                    </li>
                                    <li>
                                        <p class="about">嚴重骨質疏鬆治療</p>
                                    </li>
                                    <li>
                                        <p class="about">復健治療</p>
                                    </li>
                                    <li>
                                        <p class="about">針灸電針疼痛治療</p>
                                    </li>
                                    <li>
                                        <p class="about">肩、膝玻尿酸注射</p>
                                    </li>
                                    <li>
                                        <p class="about">骨科術後骨頭保健</p>
                                    </li>
                                    <li>
                                        <p class="about">生化雷射淨血療法</p>
                                    </li>
                                    <li>
                                        <p class="about">癌症疼痛控制治療</p>
                                    </li>
                                    <li>
                                        <p class="about">塑身體雕中西療法</p>
                                    </li>
                                    <li>
                                        <p class="about">淡斑、美白、除毛</p>
                                    </li>
                                </ul>
                            </figcaption>
                        </figure>
                    </div>
                </div>

            </div>
        </div>
    </section>
    

    <!-- SCRIPTS -->
    <script src="~/Scripts/jquery.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.sticky.js"></script>
    <script src="~/Scripts/jquery.stellar.min.js"></script>
    <script src="~/Scripts/wow.min.js"></script>
    <script src="~/Scripts/smoothscroll.js"></script>
    <script src="~/Scripts/owl.carousel.min.js"></script>
    <script src="~/Scripts/custom.js"></script>



</body>
</html>